{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "curly": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-multiple-empty-lines": 2,
    "no-undefined": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-eval": 2,
    "valid-typeof": 2
  }
}
