{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --source-maps",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile",
    "build": "gulp devBuild",
    "dev": "webpack --progress --colors --watch",
    "buildPro": "export NODE_ENV=production && gulp proBuild"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "log4js": "^0.6.33",
    "source-map-support": "0.4.0",
    "think-debug-toolbar": "^1.0.12",
    "thinkjs": "2.2.x",
    "vue": "^1.0.20",
    "vue-resource": "^0.7.0",
    "vue-strap": "^1.0.7"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-register": "^6.3.13",
    "babel-runtime": "^5.8.34",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-imagemin": "^2.4.0",
