    "gulp-sourcemaps": "^1.6.0",
    "imagemin-pngquant": "^4.2.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.2",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^8.0.2",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.0",
    "webpack-stream": "^3.1.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "css-loader": "^0.23.1",
